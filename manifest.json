{
  "manifest_version": 2,
  "name": "vscorderize",
  "description": "hack chrome",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "bookmarks"
  ],
  "icons" : {
    "16" : "src/assets/16.png",
    "48" : "src/assets/48.png",
    "128" : "src/assets/128.png"
  },
  "background": {
    "scripts": ["src/build/background.js"]
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/build/main.js"]
    }
  ]
}