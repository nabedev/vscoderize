{
  "manifest_version": 2,

  "name": "vscorderize",
  "description": "hack chrome",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "tabs",
    "history",
    "bookmarks"
  ],

  "background": {
    "scripts": ["src/build/background.js"]
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["src/build/main.js"]
    }
  ]
}